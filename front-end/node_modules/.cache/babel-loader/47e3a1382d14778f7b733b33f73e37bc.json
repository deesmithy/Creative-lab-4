OAAA,CAAA;AACA;AAJA,GAfA;AAsBA,EAAA,OAtBA,qBAsBA;AACA,SAAA,SAAA;AACA,GAxBA;AA0BA,EAAA,OAAA,EAAA;AACA,IAAA,WADA,uBACA,KADA,EACA;AACA,WAAA,IAAA,GAAA,KAAA,CAAA,MAAA,CAAA,KAAA,CAAA,CAAA,CAAA;AACA,KAHA;AAIA,IAAA,MAJA,oBAIA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAEA,gBAAA,QAFA,GAEA,IAAA,QAAA,EAFA;AAGA,gBAAA,QAAA,CAAA,MAAA,CAAA,OAAA,EAAA,MAAA,CAAA,IAAA,EAAA,MAAA,CAAA,IAAA,CAAA,IAAA;AAHA;AAAA,uBAIA,KAAA,CAAA,IAAA,CAAA,aAAA,EAAA,QAAA,CAJA;;AAAA;AAIA,gBAAA,EAJA;AAAA;AAAA,uBAKA,KAAA,CAAA,IAAA,CAAA,aAAA,EAAA;AACA,kBAAA,IAAA,EAAA,MAAA,CAAA,IADA;AAEA,kBAAA,IAAA,EAAA,EAAA,CAAA,IAAA,CAAA,IAFA;AAGA,kBAAA,GAAA,EAAA,MAAA,CAAA;AAHA,iBAAA,CALA;;AAAA;AAKA,gBAAA,EALA;AAUA,gBAAA,MAAA,CAAA,SAAA,GAAA,EAAA,CAAA,IAAA;AAVA;AAAA;;AAAA;AAAA;AAAA;AAYA,gBAAA,OAAA,CAAA,GAAA;;AAZA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAcA,KAlBA;AAmBA,IAAA,SAnBA,uBAmBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,GAAA,CAAA,aAAA,CAFA;;AAAA;AAEA,gBAAA,QAFA;AAGA,gBAAA,MAAA,CAAA,MAAA,GAAA,QAAA,CAAA,IAAA;AAHA,kDAIA,IAJA;;AAAA;AAAA;AAAA;AAMA,gBAAA,OAAA,CAAA,GAAA;;AANA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQA,KA3BA;AA4BA,IAAA,UA5BA,sBA4BA,MA5BA,EA4BA;AACA,WAAA,QAAA,GAAA,EAAA;AACA,WAAA,UAAA,GAAA,MAAA;AACA,KA/BA;AAgCA,IAAA,YAhCA,wBAgCA,MAhCA,EAgCA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,MAAA,CAAA,iBAAA,MAAA,CAAA,GAAA,CAFA;;AAAA;AAGA,gBAAA,MAAA,CAAA,UAAA,GAAA,IAAA;;AACA,gBAAA,MAAA,CAAA,SAAA;;AAJA,kDAKA,IALA;;AAAA;AAAA;AAAA;AAOA,gBAAA,OAAA,CAAA,GAAA;;AAPA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA,KAzCA;AA0CA,IAAA,UA1CA,sBA0CA,IA1CA,EA0CA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,GAAA,CAAA,iBAAA,IAAA,CAAA,GAAA,EAAA;AACA,kBAAA,KAAA,EAAA,MAAA,CAAA,UAAA,CAAA,IADA;AAEA,kBAAA,WAAA,EAAA,MAAA,CAAA,UAAA,CAAA;AAFA,iBAAA,CAFA;;AAAA;AAMA,gBAAA,MAAA,CAAA,UAAA,GAAA,IAAA;;AACA,gBAAA,MAAA,CAAA,SAAA;;AAPA,kDAQA,IARA;;AAAA;AAAA;AAAA;AAUA,gBAAA,OAAA,CAAA,GAAA;;AAVA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAYA;AAtDA;AA1BA,CAAA","sourcesContent":["<template>\n<div class=\"admin\">\n      <h1>The Admin Page!</h1>\n    <div class=\"heading\">\n      <div class=\"circle\">1</div>\n      <h2>Add a group member</h2>\n    </div>\n    <div class=\"add\">\n      <div class=\"form\">\n        <input v-model=\"name\" placeholder=\"Name\">\n        \n        <textarea v-model=\"bio\"> Description</textarea>\n        <p></p>\n        <input type=\"file\" name=\"photo\" @change=\"fileChanged\">\n        <button @click=\"upload\">Upload</button>\n      </div>\n      <div class=\"upload\" v-if=\"addPerson\">\n        <h2>{{addPerson.name}}</h2>\n        <p>{{addPerson.bio}}</p>\n        <img :src=\"addPerson.path\" />\n      </div>\n    </div>\n    <div class=\"heading\">\n      <div class=\"circle\">2</div>\n      <h2>Edit/Delete a group member</h2>\n    </div>\n    <div class=\"edit\">\n      <div class=\"form\">\n        <input v-model=\"findTitle\" placeholder=\"Search\">\n        <div class=\"suggestions\" v-if=\"suggestions.length > 0\">\n          <div class=\"suggestion\" v-for=\"s in suggestions\" :key=\"s.id\" @click=\"selectItem(s)\">{{s.title}}\n          </div>\n        </div>\n      </div>\n      <div class=\"upload\" v-if=\"findPerson\">\n        <input v-model=\"findPerson.name\">\n        <textarea v-model=\"findPerson.bio\"> Bio</textarea>\n        <p></p>\n        <img :src=\"findPerson.path\" />\n      </div>\n      <div class=\"actions\" v-if=\"findPerson\">\n        <button @click=\"deleteItem(findPerson)\">Delete</button>\n        <button @click=\"editItem(findPerson)\">Edit</button>\n\n      </div>\n    </div>\n\n</div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  name: 'Admin',\n    data() {\n    return {\n      title: \"\",\n      file: null,\n      description: \"\",\n      addPerson: null,\n      items: [],\n      findTitle: \"\",\n      findPerson: null,\n\n    }\n\n  },\n    computed: {\n    suggestions() {\n      let people = this.people.filter(person => person.name.toLowerCase().startsWith(this.findPerson.toLowerCase()));\n      return people.sort((a, b) => a.name > b.name);\n    }\n  },\n\n    created() {\n    this.getPeople();\n  },\n\n    methods: {\n    fileChanged(event) {\n      this.file = event.target.files[0]\n    },\n        async upload() {\n      try {\n        const formData = new FormData();\n        formData.append('photo', this.file, this.file.name)\n        let r1 = await axios.post('/api/photos', formData);\n        let r2 = await axios.post('/api/people', {\n          name: this.name,\n          path: r1.data.path,\n          bio: this.bio,\n        });\n        this.addPerson = r2.data;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    async getPeople() {\n  try {\n    let response = await axios.get(\"/api/people\");\n    this.people = response.data;\n    return true;\n  } catch (error) {\n    console.log(error);\n  }\n},\n    selectItem(person) {\n      this.findName = \"\";\n      this.findPerson = person;\n    },\n    async deletePerson(person) {\n      try {\n        await axios.delete(\"/api/person/\" + person._id);\n        this.findPerson = null;\n        this.getPeople();\n        return true;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    async editPerson(item) {\n      try {\n        await axios.put(\"/api/people/\" + item._id, {\n          title: this.findPerson.name,\n          description: this.findPerson.bio,\n        });\n        this.findPerson = null;\n        this.getPerson();\n        return true;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n\n  },\n\n\n}\n</script>\n\n\n\n\n\n<style scoped>\n.image h2 {\n  font-style: italic;\n  font-size: 1em;\n}\n\n.heading {\n  display: flex;\n  margin-bottom: 20px;\n  margin-top: 20px;\n}\n\n.heading h2 {\n  margin-top: 8px;\n  margin-left: 10px;\n}\n\n.add,\n.edit {\n  display: flex;\n}\n\n.circle {\n  border-radius: 50%;\n  width: 18px;\n  height: 18px;\n  padding: 8px;\n  background: #333;\n  color: #fff;\n  text-align: center\n}\n\n/* Form */\ninput,\ntextarea,\nselect,\nbutton {\n  font-family: 'Montserrat', sans-serif;\n  font-size: 1em;\n}\n\n.form {\n  margin-right: 50px;\n}\n\n/* Uploaded images */\n.upload h2 {\n  margin: 0px;\n}\n\n.upload img {\n  max-width: 300px;\n}\n/* Suggestions */\n.suggestions {\n  width: 200px;\n  border: 1px solid #ccc;\n}\n\n.suggestion {\n  min-height: 20px;\n}\n\n.suggestion:hover {\n  background-color: #5BDEFF;\n  color: #fff;\n}\n\n</style>\n"],"sourceRoot":"src/views"}]}